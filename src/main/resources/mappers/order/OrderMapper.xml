<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.green.ca2sa.order.OrderMapper">
    <insert id="insOrder">
        insert into order
            set userId = #{userId},
                cafeId = #{cafeId},
                pickUpTime = #{pickUpTime},
                memo = #{memo}
    </insert>
</mapper>