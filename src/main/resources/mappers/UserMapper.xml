<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.green.ca2sa.user.UserMapper">
    <insert id="postUser">
        INSERT INTO user
        SET  email=#{email}
            ,upw=#{upw}
            ,nickName=#{nickName}
            ,phone=#{phone}
            ,agree=#{agree}
    </insert>

    <select id="getUserEmailCheck">
        SELECT email
        FROM user
        WHERE email=#{email}
    </select>



</mapper>